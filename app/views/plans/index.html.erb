<p id="notice"><%= notice %></p>

<h1>Listing Plans</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Warm up</th>
      <th>Intro</th>
      <th>Drill</th>
      <th>Practice</th>
      <th>Gamesandactivities</th>
      <th>Review</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plans.each do |plan| %>
      <tr>
        <td><%= plan.title %></td>
        <td><%= plan.start_time %></td>
        <td><%= plan.end_time %></td>
        <td><%= plan.warm_up %></td>
        <td><%= plan.intro %></td>
        <td><%= plan.drill %></td>
        <td><%= plan.practice %></td>
        <td><%= plan.gamesandactivities %></td>
        <td><%= plan.review %></td>
        <td><%= link_to 'Show', plan %></td>
        <td><%= link_to 'Edit', edit_plan_path(plan) %></td>
        <td><%= link_to 'Destroy', plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plan', new_plan_path %>
